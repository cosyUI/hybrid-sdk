{
  "name": "wya-js-sdk",
  "version": "0.1.0",
  "description": "",
  "main": "dist/wya.umd.js",
  "files": [
    "src/**",
    "dist/**",
    "examples/**"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run serve",
    "serve": "rm -rf dist/ && cross-env NODE_ENV=development BABEL_ENV=rollup rollup --config ./config/rollup.config.js -w",
    "build": "rm -rf dist/ ../../pre-build && cross-env NODE_ENV=production BABEL_ENV=rollup rollup --config ./config/rollup.config.js && cp-cli ./dist ../../pre-build",
    "lint": "esw *.config.* src",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "all": "npm-run-all --parallel --race --continue-on-error lint:watch serve",
    "lint-staged": "lint-staged",
    "pre-commit:install": "cp .hooks/* .git/hooks/",
    "test": "cross-env BABEL_ENV=default jest --config ./tests/config/jest.config.js --watch"
  },
  "author": "wya-team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wya-team/hybrid-sdk"
  },
  "bugs": {
    "url": "https://github.com/wya-team/hybrid-sdk/issues"
  },
  "dependencies": {
    "wya-ps": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cp-cli": "^1.1.2",
    "cross-env": "^5.1.3",
    "enzyme": "^3.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.12.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-watch": "^3.1.3",
    "fs-extra": "^5.0.0",
    "jest": "^23.5.0",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-watch": "^4.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
